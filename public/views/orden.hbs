{{#if existe}}
<h2>Orden Recibida</h2>
<p class="datos_carritoOrden">numero de orden: {{order.orden_numero}}</p>

<h4 class="datos_carritoOrden">usuario:  {{order.username}}</h4>
<p class="datos_carritoOrden"> direcci√≥n:  {{order.direccion}}</p>
<ul>
{{#each order.items}}
<li class="items_carritoOrden"> <strong>{{this.nombre}}</strong>  <p>Precio:${{this.precio}}</p>  <p>cantidad:{{this.cantidad}}</p>  <strong>SUBTOTAL: ${{this.sub}}</strong> </li>
{{/each}}
<strong class="total">TOTAL: $ {{order.total}}</strong>
</ul>
{{else}}
<h4>La orden no existe</h4>
{{/if}}
<a href="/api/productos">seguir viendo</a>
<a href="/api/login/logout">abandonar pagina</a>

